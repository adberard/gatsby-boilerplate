{
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,css,md,yml}\"",
    "lint": "yarn run lint:es",
    "lint:fix": "yarn run lint:es --fix",
    "lint:es": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:stats": "node node_modules/eslint/bin/eslint.js --format node_modules/eslint-stats/byErrorAndWarning.js src/**/*.{ts,tsx} -c .eslintrc.js",
    "type:check": "tsc --noEmit",
    "type:gen": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/types/graphql.d.ts",
    "cz": "git-cz",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "release": "standard-version",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
