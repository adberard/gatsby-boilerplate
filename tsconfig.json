{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@src": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@apollo/*": ["./src/apollo/*"],
      "@ducks/*": ["./src/ducks/*"],
      "@configs/*": ["./src/configs/*"],
      "@assets/*": ["./src/assets/*"],
      "@styles/*": ["./src/styles/*"],
      "@containers/*": ["./src/containers/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@utils/*": ["./src/utils/*"],
      "@pages/*": ["./src/pages/*"]
    }
  },
  "include": [
    "src/**/*"
  ]
}
